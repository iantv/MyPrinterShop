<!DOCTYPE HTML>
<html>
<head>
<title>{{ page_title }}</title>
<meta http-equiv="content-type" content="text/html; charset=UTF-8" />
{% stylesheets 
	'bundles/app/css/jqGrid/css/ui.jqgrid.css'
	'bundles/app/css/jquery-ui/themes/hot-sneaks/jquery-ui.css'
	'bundles/app/css/main_styles.css'
 	filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}
{{ db|raw }}
{% javascripts '@jquery_and_jqGrid' %}
	<script src="{{ asset_url }}"></script>
{% endjavascripts %}
</head>
<body>
	<img src=""></img>
	<ul class="entrance">
		<li><a href="/register">Регистрация</a></li>
		<li><a href="registration">Вход</a></li>
	</ul>
	<div id="header">
		<div class="site_name_block">
			<span class="site_name">{{ site_name }}<span><br>
			<span class="site_phrase">Всё для принтера</span>
		</div>
		{% image '@AppBundle/Resources/public/images/printer.png' %}
		    <img src="{{ asset_url }}"/>
		{% endimage %}
	</div>
	<div id="content">
		<div id="menu_block">
		{{ knp_menu_render('AppBundle:Builder:mainMenu') }}
		</div>
		<div id="grid_block">
		<h4>{{ path|raw }}</h4>
			<div id="pager"></div>
			<table id="list"><tr><td></td></tr></table>
		    <div id="pager"></div>
	    </div>
    </div>
    {% javascripts '@AppBundle/Resources/public/js/jqgrid_create.js'
    	'@AppBundle/Resources/public/js/generate_menu.js' %}
		<script src="{{ asset_url }}"></script>
	{% endjavascripts %}
</body>
</html>
